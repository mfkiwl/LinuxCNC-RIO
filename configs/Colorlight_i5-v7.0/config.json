{
    "name": "Colorlight_i5-v7.0",
    "description": "only for testing",
    "toolchain": "icestorm",
    "family": "ecp5",
    "type": "25k",
    "package": "CABGA381",
    "transport": "SPI",
    "clock": {
        "_osc": "25000000",
        "speed": "25000000",
        "pin": "P3"
    },
    "blink": {
        "pin": "U16"
    },
    "plugins": [
        {
            "type": "picosoc",
            "baud": 115200,
            "peripherals": [
                {
                    "type": "gpio",
                    "pins": {"gpio0": ["H5", "OUTPUT"], "gpio1": ["J5", "OUTPUT"]}
                },
                {
                    "type": "counter"
                },
                {
                    "type": "sysclock"
                }
            ],
            "pins": {
                "rx": "A9",
                "tx": "D10",
                "flash_clk": "L16",
                "flash_csb": "L13",
                "flash_io0": "K17",
                "flash_io1": "K18",
                "flash_io2": "L14",
                "flash_io3": "M14"
            },
            "name": "PICOSOC"
        }

    ]
}
