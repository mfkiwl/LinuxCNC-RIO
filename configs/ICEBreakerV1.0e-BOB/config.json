{
    "name": "ICEBreakerV1.0e-BOB",
    "description": "Small and low cost FPGA educational and development board",
    "toolchain": "icestorm",
    "family": "ice40",
    "type": "up5k",
    "package": "sg48",
    "flashcmd": "openFPGALoader -b ice40_generic rio.bin -f",
    "clock": {
        "osc": "12000000",
        "speed": "6000000",
        "_speed": "12000000",
        "pin": "35"
    },
    "interface": [
        {
            "type": "spi",
            "pins": {
                "MOSI": "27",
                "MISO": "25",
                "SCK": "21",
                "SEL": "19"
            }
        }
    ],
    "blink": {
        "pin": "37"
    },
    "error": {
        "invert": true,
        "pin": "11"
    },
    "plugins": [
        {
            "pullup": true,
            "pin": "38",
            "name": "DIN0",
            "type": "din_bit"
        },
        {
            "pullup": true,
            "pin": "34",
            "name": "DIN1",
            "type": "din_bit"
        },
        {
            "pullup": true,
            "pin": "31",
            "name": "DIN2",
            "type": "din_bit"
        },
        {
            "pullup": true,
            "pin": "48",
            "name": "DIN3",
            "type": "din_bit"
        },
        {
            "type": "joint_stepper",
            "cl": false,
            "pins": {
                "step": "2",
                "dir": "47"
            },
            "name": "JOINT0"
        },
        {
            "type": "picosoc",
            "baud": 115200,
            "mem": 1024,
            "prog_addr": "0x00200000",
            "pins": {
                "rx": "6",
                "tx": "9",
                "flash_clk": "15",
                "flash_csb": "16",
                "flash_io0": "14",
                "flash_io1": "17",
                "flash_io2": "12",
                "flash_io3": "13"
            },
            "name": "PICOSOC"
        }
    ]
}
